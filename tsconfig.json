{
	"compilerOptions": {
		"target": "esnext",
		"lib": [
			"esnext"
		],
		"rootDir": ".",
		"module": "esnext",
		"moduleResolution": "Bundler",
		"resolveJsonModule": true,
		"allowJs": true,
		"strict": true,
		"strictNullChecks": true,
		"esModuleInterop": true,
		"skipDefaultLibCheck": true,
		"skipLibCheck": true
	},
	"include": [
		".vitepress/**/*"
	],
	"exclude": [
		"dist",
		"node_modules"
	]
}
