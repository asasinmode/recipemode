{
  "compilerOptions": {
    "target": "esnext",
    "rootDir": ".",
    "module": "esnext",
    "lib": [
      "esnext"
    ],
    "moduleResolution": "Bundler",
    "esModuleInterop": true,
    "allowJs": true,
    "strict": true,
    "strictNullChecks": true,
    "resolveJsonModule": true,
    "skipLibCheck": true,
    "skipDefaultLibCheck": true
  },
  "exclude": [
    "dist",
    "node_modules"
  ],
  "include": [
    ".vitepress/**/*"
  ]
}
